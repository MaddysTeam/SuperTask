@model List<PMScoreDetailViewModel>
<div class="modal-dialog">

	<!-- widget -->
	<div class="widget">
		<div class="widget-content">

			<table id="bootgrid2" class="table table-striped table-hover table-dark-header">
				<thead>
					<tr>
						<th>用户名</th>
						<th>任务名称</th>
						<th>项目名称</th>
						<th>预估开始</th>
						<th>预估结束</th>
						<th>实际开始</th>
						<th>实际结束</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>@item.UserName</td>
							<td>@item.TaskName</td>
							<td>@item.ProjectName</td>
							<td>@item.ESDate</td>
							<td>@item.EEDate</td>
							<td>@item.RSDate</td>
							<td>@item.REDate</td>
						</tr>
					}
				</tbody>
			</table>

		</div>

	</div>

</div>
