<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<head>
	<title>@ViewBag.Title</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="description" content="项目管理系统">
	<meta name="author" content="上海教育软件有限公司">

	<!-- css -->
	<link href="/assets/css/bootstrap.min.css" rel="stylesheet" />
	<link href="/assets/css/font-awesome.min.css" rel="stylesheet" />
	<link href="/assets/css/main.min.css" rel="stylesheet" />
	<link href="/assets/css/app.css" rel="stylesheet" />

	@RenderSection("Css", required: false)

	<!-- /css -->
	<!--[if lte IE 9]>
		<script src="/assets/js/respond.min.js"></script>
		<script src="/assets/js/html5shiv.min.js"></script>
	<![endif]-->
</head>
<body class="full-page">
	<!-- wrapper -->
	<div class="wrapper">

		<!-- top bar -->
		<div class="top-bar">
			<div class="container">
				<div class="row">
					<!-- logo -->
					<div class="col-md-1">
						<a href="@Url.Action("Index", "Home")">
							@*<img src="~/assets/img/trainingadmin-logo-white.png" alt="学籍管理系统" />*@
						</a>
						@*<div style="color:white;font-size:16px;font-weight:bolder">项目管理系统</div>*@
					</div>
					<!-- /logo -->
					<div class="col-md-11">
						<div class="row">
							<div class="col-md-9">
								<div class="top-bar-left">
									@Html.Partial("_nav")
								</div>
							</div>
							<div class="col-md-3">
								<!-- top-bar-right -->
								<div class="top-bar-right">
									<!-- responsive menu bar icon -->
									<a href="#" class="hidden-md hidden-lg main-nav-toggle"><i class="fa fa-bars"></i></a>
									<!-- /responsive menu bar icon -->

									@Html.Partial("_notice")
									@Html.Partial("_user")
								</div>
								<!-- /top-bar-right -->
							</div>
						</div>
						<!-- /row -->
					</div>
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /top bar -->
		<!-- bottom: left nav and right main content -->
		<div class="bottom">
			<div class="container">
				<div class="row">
					@*@Html.Partial("_sidebar",ViewBag.SidebarModels as List<MenuModel>)*@


					<!-- content-wrapper -->
					@RenderBody()
					<!-- /content-wrapper -->
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /bottom: left nav and right main content -->

		<div class="push-sticky-footer"></div>
	</div>
	<!-- /wrapper -->
	<!-- footer -->
	<footer class="footer">
		&copy;上海教育软件有限公司
	</footer>
	<!-- /footer -->
	@*@Html.Partial("_styleSwitcher")*@
	@RenderSection("Modals", required: false)

	<!-- javascript -->
	<script src="/assets/js/jquery-1.11.1.min.js"></script>
	<script src="/assets/js/bootstrap.min.js"></script>
	<script src="/assets/js/modernizr-2.6.2.min.js"></script>
	<script src="/assets/js/jqueryval-mis.min.js"></script>
	<script src="/assets/js/plugins.min.js"></script>
	@RenderSection("Plugins", required: false)
	<script src="/assets/js/king-common.min.js"></script>
	<script src="/assets/js/app.js"></script>
	@RenderSection("Scripts", required: false)
	<!-- /javascript -->
</body>
</html>
