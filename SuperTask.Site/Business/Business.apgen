					<add name="Note"  columnName="Note" type="string" isNullable="true"  dataLength="8000" display="备注"></add>
					<add name="RoleType"  columnName="Type" type="int" isNullable="true"  comment="角色类型"></add>

				</columns>
			</table>

			<table name="UserRole" tableName="UsersRole" comment="用户角色">
				<columns>

					<add name="ID" columnName="ID" type="Guid" primaryKey="true"></add>
					<add name="UserId" columnName="MemberID" type="Guid" ></add>
					<add name="RoleId" columnName="RoleID" type="Guid"  ></add>
					<add name="IsDefault" type="int" ></add>

				</columns>
			</table>

