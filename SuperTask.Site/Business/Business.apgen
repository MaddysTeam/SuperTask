					<add name="Progress" columnName="progress" type="double" isNullable="true"  display="项目进度"/>
					<add name="RecordDate" columnName="RecordDate" type="DateTime" isNullable="true" display="记录时间"/>
					<add name="ProcessName" type="string"  dataLength="100"   isNullable="true" display="项目进展" ></add>
					<add name="ProjectStatus" columnName="ProjectStatus" type="Guid" isNullable="true" display="项目状态" />
					<add name="ProjectType" columnName="ProjectType" type="Guid" isNullable="true" display="项目类型" />
					<add name="RateOfProgress" columnName="RateOfProgress" type="double" isNullable="true"  display="项目进度"/>
					<add name="PMId" columnName="PMId" type="Guid"  isNullable="true" display="项目负责人ID"/>
					<add name="StartDate"  columnName="StartDate" type="DateTime" isNullable="true" defaultValue="DateTime.Now" display="预估开始时间" />
					<add name="EndDate"  columnName="EndDate" type="DateTime" isNullable="true" display="预估结束时间" />
					<add name="ProjectOwner" columnName="ProjectOwner" type="string" dataLength="100"  isNullable="true" display="项目甲方" />
					<add name="RealCode" columnName="RealCode"  type="string" dataLength="100" isNullable="true" display="立项编号"></add>
					<add name="ReviewerId" columnName="ReviewerId" type="Guid"  isNullable="true" display="审核人ID" />
					<add name="ProjectExecutor" columnName="ProjectExecutor"  type="string" dataLength="100"  isNullable="true" display="项目乙方"></add>
					<add name="ManagerId" columnName="ManagerId" type="Guid"  isNullable="true" display="项目经理"/>
					<add name="Code" columnName="Code" type="string" dataLength="100"  isNullable="true" display="内部项目编号"/>
					<add name="OperatorId" columnName="OperatorId" type="Guid"  isNullable="true" display="操作人"/>

				</columns>
			</table>

			<table name="ProjectRole" tableName="ProjectRole" comment="项目角色">
				<columns>

					<add name="PRID" columnName="ID" type="Guid" primaryKey="true"></add>
					<add name="ProjectId" columnName="ProjectID" type="Guid" ></add>
					<add name="RoleId" columnName="RoleID" type="Guid"  ></add>
					<add name="IsDefault" type="int" dataLength="1" defaultValue="1" comment="是否是默认角色 1:是 0 否" ></add>
					<add name="AppIds"  columnName="AppIds" type="string" dataLength="2000" isNullable="true"  display="项目角色权限" comment="权限Id用逗号分开"></add>

				</columns>
			</table>

			<table name="Resource" comment="项目资源">
				<columns>

					<add name="ResourceId" columnName="ResourceId" type="Guid" primaryKey="true" comment="资源ID"/>
					<add name="UserId" columnName="UserId" type="Guid" comment="资源人ID"/>
					<add name="Projectid" columnName="Projectid" type="Guid" isNullable="true"  comment="项目ID" />
					<add name="TaskId" columnName="TaskId" type="Guid"  isNullable="true"  comment="任务ID"/>
					<add name="ResourceName"  columnName="Name" type="string" dataLength="100"  display="资源名称" ></add>
					<add name="ResourceTypes"  columnName="Types" type="string" dataLength="200" isNullable="true"  display="资源类型" comment="资源角色用逗号分开"></add>
					<add name="Description" columnName="Description" type="string" dataLength="1000" isNullable="true"  display="资源描述" />
					<add name="CreateDate" columnName="CreateDate" type="DateTime" isNullable="true" defaultValue="DateTime.Now"  display="创建时间" />
					<add name="ModifyDate" columnName="ModifyDate" type="DateTime" isNullable="true" display="修改时间" />
					<add name="Status" columnName="Status" type="Guid"  isNullable="true"  display="资源状态" ></add>

				</columns>
			</table>
			
			<table name="Payments" comment="款项">
				<columns>
					<add name="PayId" columnName="ID" type="Guid" primaryKey="true" comment="款项ID"/>
					<add name="PayName" columnName="PayName" type="string" dataLength="100" display="款项名称"  comment="款项名称"/>
