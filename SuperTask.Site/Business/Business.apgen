					<add name="CompanyStatus" type="string" dataLength="40" required="true" display="状态" />
				</columns>

			</table>

			<!-- Project & Resource -->

			<table name="Project" comment="项目">
				<columns>

					<add name="ProjectId" columnName="ID" type="Guid"  primaryKey="true" display="项目ID" />
					<add name="ProjectName" columnName="Name" type="string" dataLength="200"  required="true" display="项目名称"/>
					<add name="Description" columnName="Description" type="string" dataLength="40000"  isNullable="true" display="项目描述"/>
					<add name="ProjectStatus" columnName="Status" type="Guid" isNullable="true" display="项目状态" />
					<add name="ProjectType" columnName="Type" type="Guid" isNullable="true" display="项目类型" />
					<add name="RateOfProgress" columnName="progress" type="double" isNullable="true"  display="项目进度"/>
					<add name="PMId" columnName="PMId" type="Guid"  isNullable="true" display="项目负责人ID"/>
					<add name="CreatorId" columnName="CreateorId" type="Guid"  isNullable="true"  display="项目创建者ID"/>
					<add name="StartDate"  columnName="StartDate" type="DateTime" isNullable="true" defaultValue="DateTime.Now" display="预估开始时间" />
					<add name="EndDate"  columnName="EndDate" type="DateTime" isNullable="true" display="预估结束时间" />
					<add name="RealStartDate"  columnName="RealStartDate" type="DateTime"  isNullable="true" display="实际开始时间" />
					<add name="RealEndDate"  columnName="RealEndDate" type="DateTime"  isNullable="true" display="实际结束时间" />
					<add name="ProjectOwner" columnName="Owner" type="string" dataLength="100"  isNullable="true" display="项目甲方" />
					<add name="CreateDate" columnName="CreateDate" type="DateTime"  isNullable="true"  display="创建时间" />
					<add name="ModifyDate" columnName="ModifyDate" type="DateTime"  isNullable="true" display="最近修改时间" />
					<add name="OrgId" columnName="OrgId" type="Guid" isNullable="true" display="项目归属部门"/>
					<add name="Code" columnName="Code" type="string" dataLength="100"  isNullable="true" display="内部项目编号"/>
					<add name="RealCode" columnName="RealCode"  type="string" dataLength="100" isNullable="true" display="立项编号"></add>
					<add name="ReviewerId" columnName="ReviewerId" type="Guid"  isNullable="true" display="审核人ID" />
					<add name="ProjectExecutor" columnName="Executor"  type="string" dataLength="100"  isNullable="true" display="项目乙方"></add>
					<add name="ProjectExecutorId" columnName="ExecutorId"  type="Guid" isNullable="true" display="项目乙方"></add>
					<add name="ManagerId" columnName="ManagerId" type="Guid"  isNullable="true" display="项目经理"/>
					<add name="ProcessName" type="string"  dataLength="100"   isNullable="true" display="项目进展" />
					<add name="FolderId" columnName="FolderId" type="Guid" isNullable="true" display="项目文件夹ID"   />
					<add name="Money" columnName="money" type="double" isNullable="true"  display="预估总金额"/>
					<add name="CMoney" columnName="cmoney" type="double" isNullable="true"  display="合同总金额"/>
					<add name="Coefficient" columnName="Coefficient" type="double" isNullable="true" display="项目难度系数" />
				</columns>
			</table>

			<table name="ProjectRecord" comment="项目记录">
				<columns>

					<add name="RecordId" columnName="ID" type="Guid"  primaryKey="true" display="日志ID" />
					<add name="ProjectName" columnName="ProjectName" type="string" dataLength="200"  required="true" display="项目名称"/>
