					<add name="ParentId" columnName="ParentId" type="Guid"  isNullable="true"  comment="父级ID"/>
					<add name="Content" columnName="Description" type="string" dataLength="2000"  isNullable="true" display="内容"/>
					
				</columns>
			</table>

			<table name="MileStone">
				<columns>
					<add name="StoneId" columnName="ID" type="Guid" primaryKey="true" comment="节点ID"/>
					<add name="StoneName"  type="string" dataLength="100"  display="节点名称" ></add>
					<add name="StoneStatus"  type="Guid"   display="节点状态"/>
					<add name="StoneType"  type="Guid"   display="节点类型"/>
