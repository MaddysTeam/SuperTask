	<businessModel autoSyncDatabase="true" autoInitDatabase="true">

		<tables>

			<!-- Account & User -->

			<table name="Account" tableName="Users" comment="用户账号">
				<columns>

					<add name="UserId" columnName="ID" type="Guid" primaryKey="true"  comment="用户ID"/>
					<add name="UserName"  columnName="Name" type="string" dataLength="40" required="true" display="用户名" />
					<add name="Account" type="string" dataLength="40" required="true" display="账号" />
					<add name="Password" type="string" dataLength="40" required="true" display="密码" />
					<add name="Status" type="int" isNullable="true"  defaultValue="0" display="账号状态" />
					<add name="Sort" type="int" isNullable="true" defaultValue="0"  display="账号排序号" />
					<add name="Note" type="string" isNullable="true"  dataLength="1000" display="备注" />

				</columns>
			</table>

			<table name="UserInfo" tableName="UserInfo" comment="用户信息">
				<columns>

					<add name="UserId" columnName="ID" type="Guid" primaryKey="true"  comment="用户ID"/>
					<add name="UserName" type="string" dataLength="40" required="true" display="用户名" />
					<add name="RealName" type="string" dataLength="40" isNullable="true"  display="真实姓名" />
					<add name="NickName" type="string" dataLength="40" isNullable="true" display="用户昵称" />
					<add name="Gender" type="int"  display="性别" />
					<add name="Department" type="string" dataLength="255" isNullable="true" display="所在部门" />
					<add name="Position" type="string" dataLength="255" isNullable="true" display="职位" />
					<add name="Phone" type="string" dataLength="20" isNullable="true" display="公司电话" />
					<add name="Mobile" type="string" dataLength="20" isNullable="true" display="手机号" />
					<add name="Email" type="string" dataLength="50" isNullable="true" display="邮箱" />
					<add name="Birthday" type="DateTime?"  isNullable="true" display="生日" />
					<add name="Introduce" type="string" dataLength="500" isNullable="true" display="自我介绍"/>
					<add name="IsDelete" type="bool" isNullable="true" />

				</columns>
			</table>


			<!-- Role & Permission-->

			<table name="App" tableName="AppLibrary" comment="应用程序库(权限)">
				<columns>

					<add name="AppId" columnName="ID" type="Guid" primaryKey="true" ></add>
					<add name="Title"  columnName="Title" type="string" dataLength="255" display="标题"></add>
					<add name="Address" columnName="Address" dataLength="255" type="string"  display="应用地址"></add>
					<add name="AppType" columnName="Type" type="Guid"></add>
					<add name="Code"  columnName="Code" type="string" dataLength="200" isNullable="true"></add>
					<add name="Note"  columnName="Note" type="string" dataLength="200" isNullable="true" display="应用描述"></add>

					<!-- adapt work flow -->
					<add name="backup1"  columnName="OpenMode"  type="int" isNullable="true"></add>
					<add name="backup2"  columnName="Width"  type="int" isNullable="true" ></add>
					<add name="backup3" columnName="Height" type="int" isNullable="true" ></add>
					<add name="backup4"  columnName="Params" type="string" dataLength="2000" isNullable="true"></add>
					<add name="backup5"  columnName="Manager" type="string" dataLength="2000" isNullable="true" ></add>
					<add name="backup6"  columnName="UseMember" type="string" dataLength="2000" isNullable="true" ></add>

				</columns>
			</table>

			<table name="Role" tableName="Role" comment="角色">
				<columns>

					<add name="RoleId" columnName="ID" type="Guid" primaryKey="true"></add>
					<add name="RoleName"  columnName="Name" type="string" dataLength="200" display="角色名称"></add>
