			<!-- Task -->

			<table name="WorkTask"  comment="工作任务">
				<columns>

					<add name="TaskId" columnName="ID" type="Guid"  primaryKey="true" display="任务ID" />
					<add name="Projectid" columnName="ProjectId" type="Guid" display="项目ID" />
					<add name="TaskName" columnName="Name" type="string"  dataLength="200" required="true" display="任务名称"/>
					<add name="Description" columnName="Description" type="string" dataLength="1000"  isNullable="true" display="任务描述"/>
					<add name="TaskType"  columnName="Type" type="Guid"  isNullable="true"   display="任务类型" />
					<add name="TaskLevel"  columnName="Level" type="int"  isNullable="true"  defaultValue="1" display="任务级别" />
					<add name="TaskStatus"  columnName="Status" type="Guid"  isNullable="true"  display="任务状态" />
					<add name="RateOfProgress" columnName="Progress" type="double" defaultValue="0"  display="任务进度"/>
					<add name="ParentId"  columnName="ParentId"  type="Guid"  isNullable="true" display="父任务ID" />
					<add name="WorkHours"  columnName="WorkHours" type="double"  isNullable="true" display="已用工时" />
					<add name="EstimateWorkHours"  columnName="EstimateWorkHours" type="double"  isNullable="true" display="预计工时" />
					<add name="ManagerId" columnName="ManagerId" type="Guid"  isNullable="true"  display="任务负责人ID" />
					<add name="CreatorId" columnName="CreatorId" type="Guid"   isNullable="true" display="任务创建者ID" />
					<add name="ReviewerID" columnName="ReviewerId" type="Guid"   isNullable="true" display="任务审核者ID" />
					<add name="StartDate"  columnName="StartDate" type="DateTime"  isNullable="true" defaultValue="DateTime.Now" display="开始时间" />
					<add name="EndDate"  columnName="EndDate" type="DateTime"  isNullable="true" defaultValue="DateTime.Now"  display="结束时间" />
					<add name="RealStartDate"  columnName="RealStartDate" type="DateTime"  isNullable="true" display="实际开始时间" />
					<add name="RealEndDate"  columnName="RealEndDate" type="DateTime"  isNullable="true" display="实际结束时间" />
					<add name="CreateDate" columnName="CreateDate" type="DateTime" defaultValue="DateTime.Now"  display="创建时间" />
					<add name="CloseDate"  columnName="CloseDate" type="DateTime"  isNullable="true" display="任务关闭时间" />
					<add name="ModifyDate" columnName="ModifyDate" type="DateTime"  isNullable="true"  display="最近修改时间" />
					<add name="SortId" columnName="SortId" type="int" isNullable="true" comment="排序Id" />
					<add name="IsParent"  columnName="IsParent" type="bool" comment="是否是父任务" ></add>

					<!--用于2020新版本-->
					<add name="V2Level" columnName="V2Level" type="Guid" isNullable="true" comment="排序Id" />

					<!--该字段用于文档归类，体现在每个项目管理中的文件管理板块-->
					<add name="TaskFileType"  columnName="FileType" type="Guid"  isNullable="true"  display="文档类型" />

					<!--该字段只有运维任务用到-->
					<add name="ServiceCount" columnName="ServiceCount" type="int" isNullable="true" display="运维数量" comment="（该字段只为运维任务创建）"></add>

					<!--该字段在考核模块和复杂度管理板块用到-->
					<add name="StandardItemId" columnName="StandardItemId" isNullable="true" type="Guid" display="标准复杂度" comment="标准项ID" />

					<!--该字段在任务子类型功能中用到，可能用于将来的自动考核-->
					<add name="SubTypeId"  columnName="SubTypeId" type="Guid"  isNullable="true" display="子类型"/>
					<add name="SubTypeValue"  columnName="SubTypeValue" type="double"  isNullable="true"  defaultValue="0" display="任务子类型值" />

				</columns>
			</table>

